local success, response = pcall(function()
    return game:HttpGet(api_link)
end)

if not success then
    player:Kick("‚ùå DuyHub: L·ªói m·∫°ng! Kh√¥ng th·ªÉ k·∫øt n·ªëi ƒë·∫øn m√°y ch·ªß.")
